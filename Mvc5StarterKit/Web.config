    <add key="izenda.cache.memcache.systemcache.maxmemusage" value="256"/>
    <!-- Unit: MB; System cache: >= 256 MB, default: 256MB-->
    <add key="izenda.cache.diskcache.datacachedirectory" value="Caches/DataCache"/>
    <add key="izenda.cache.diskcache.systemcachedirectory" value="Caches/SystemCache"/>
    <add key="izenda.cache.data.category" value="Schedule"/>
    <add key="izenda.cache.system.category" value="Schedule"/>
    <!-- Disk cache defulat enable -->
    <add key="izenda.cache.data.cachestore" value="ExternalStorageCacheStore"/>
    <add key="izenda.cache.system.cachestore" value="ExternalStorageCacheStore"/>
    <!-- to enable disk cache please comment external storage cache -->
    <!--<add key="izenda.cache.data.cachestore" value="MemoryCacheStore"/> 
    <add key="izenda.cache.system.cachestore" value="MemoryCacheStore"/>-->    
    <!--Izenda End-->

